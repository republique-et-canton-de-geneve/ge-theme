{
  "$schema": "https://turbo.build/schema.json",
  "envMode": "strict",
  "globalEnv": ["REBUILD_HISTORICAL"],
  "globalDependencies": ["package.json", "yarn.lock"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "../../build/**"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
